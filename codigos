// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int paulo;
    int jose;
    int soma;
    int multiplicacao;
    paulo = -54;
    jose = 24;
    soma = paulo + jose;
    multiplicacao = paulo * jose;
    printf("o valor de paulo é %d \n" , paulo);
    printf("o valor de jose é %d \n" , jose);
    printf("a soma é %d \n" , soma);
    printf("a multiplicacao é %d" , multiplicacao);
    
    

    return 0;
}
